unit uUser;

interface

type TUser = class

  private
    FId: string;
    FName: string;
    FEmail: String;
    FPassword: string;
  public
    property Id: string read FId write FId;
    property Name: string read FName write FName;
    property Email: String read FEmail write FEmail;
    property Password: string read FPassword write FPassword;

end;

implementation

end.
