unit uSearchFilters;

interface

type TSearchFilters = class
  private
    FValueSearch: string;
    FOrderBy: Int8;
    FFieldSearch: string;

  public
    property ValueSearch: string read FValueSearch write FValueSearch;
    property OrderBy: Int8 read FOrderBy write FOrderBy;
    property FieldSearch: string read FFieldSearch write FFieldSearch;
end;

implementation

end.
